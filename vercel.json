{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "livek.one"
        }
      ],
      "destination": "https://livekone.com/$1",
      "permanent": true
    }
  ]
}
